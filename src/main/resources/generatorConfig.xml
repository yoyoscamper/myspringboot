<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry location="F://mysql-connector-java-5.1.7-bin.jar"/>
    <context id="mysqlgenerator" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/test?useUnicode=true&amp;characterEncoding=utf-8&amp;useSSL=false"
                        userId="root"
                        password="123456" />

        <javaModelGenerator targetPackage="com.ydy.scamper.entity" targetProject="src/main/java" />

        <sqlMapGenerator targetPackage="com.ydy.scamper.mapper" targetProject="src/main/java" />

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.ydy.scamper.mapper" targetProject="src/main/java" />



        <table tableName="user"/>

    </context>


</generatorConfiguration>